            <kind>1</kind>
            <string>/// Process Player Input/Movement
event_inherited()

depth = -y-2

var movex = 0
var movey = 0

if(global.canMove) {
  if(keyboard_check(vk_left)) movex = -move_speed 
  if(keyboard_check(vk_right)) movex = move_speed
  if(keyboard_check(vk_up)) movey = -move_speed
  if(keyboard_check(vk_down)) movey = move_speed
}

// debug 
if(global.debug) {

  // toggle fast walking
  if(keyboard_check_pressed(vk_space)) global.fast = !global.fast
  
  // increment energy balllls
  if(keyboard_check_pressed(ord('2'))) global.energy++
  
  // quicken movespeed cuz H4XX0RZ
  if(global.fast) {
    movex *= 2
    movey *= 2
  }
}


if(place_free(x + movex, y )) x += movex
else movex = 0

if(place_free(x, y + movey)) y += movey
else movey = 0

// don't animate if against walls 
if(movex == 0 &amp;&amp; movey == 0) {
  image_speed = 0
  image_index = 0
} else image_speed = 0.3

// limit view to current map tile
view_xview[0] = floor(x/64)*64
view_yview[0] = floor(y/64)*64


</string>
